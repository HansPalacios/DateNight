<div class="createvenue" class="row">
	<div id="close3">x</div>
	<%= form_with(model: @venue, local: true) do |form| %>
	  <% if @venue.errors.any? %>
	    <div id="error_explanation">
	      <h2><%= pluralize(venue.errors.count, "error") %> prohibited this venue from being saved:</h2>

	      <ul>
	      <% @venue.errors.full_messages.each do |message| %>
	        <li><%= message %></li>
	      <% end %>
	      </ul>
	    </div>
	  <% end %>
	  <div class="row">
	  	<h3>New Venue</h3>
	  </div>
	  <div class="field row">
	    <%= form.label :name %>
	    <%= form.text_field :name, id: 'placename'%>
	  </div>

	  <div class="field row">
	    <%= form.label :bio %>
	    <%= form.text_area :bio, id: :venue_bio %>
	  </div>

	  <div class="field row">
	    <%= form.label :address %>
	    <%= form.text_field :address, id: "place-address" %>
	  </div>

	  <div class="field row">
	    <%= form.label :phone %>
	    <%= form.text_field :phone, id: "placephone" %>
	  </div>

	  <div class="field row">
	    <%= form.label :rating_id %>
	    <%= form.number_field :rating_id, id: :venue_rating_id %>
	  </div>

	  <div class="field row">
	    <%= form.label :google_rating %>
	    <%= form.number_field :google_rating, id: "placerating" %>
	  </div>

	   <div class="field row">
	    <%= form.label :yelp_rating %>
	    <%= form.number_field :yelp_rating %>
	  </div>

	  <div class="row">
	  	<%= form.label "Image: " %>
	    <%= form.file_field :image, :id => "image" %>
	  </div>

	  <div class="field row">
	  	<div class="col-md-4 checklist">
	  		<div class="row">
	  			<h4>Bars</h4>
	  		</div>
	  		<div class="row">
	  			<%= form.label :music %>
	  			<%= form.check_box :music, id: :venue_music %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :dancing %>
	  			<%= form.check_box :dancing, id: :venue_dancing %>
	  		</div>
	  		<div class="row">
	  		<%= form.label :rooftop %>
	  			<%= form.check_box :rooftop, id: :venue_rooftop %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :relaxed %>
	  			<%= form.check_box :relaxed, id: :venue_relaxed %>
	  		</div>
	  	</div>
	  	<div class="col-md-4 border checklist">
	  		<div class="row">
	  			<h4>Restaurants</h4>
	  		</div>
	  		<div class="row">
	  			<%= form.label "Suit & Tie" %>
	  			<%= form.check_box :Suit_and_Tie, id: 8  %>
	  		</div>
	  		<div class="row">
	  			<%= form.label "Classy & Casual" %>
	  			<%= form.check_box :Classy_and_Casual, id: 9 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label "Hipster & Biergartens" %>
	  			<%= form.check_box :Hipster_and_biergartens, id: 10 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :Quick_Bite %>
	  			<%= form.check_box :Quick_Bite, id: 11 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :Vegan %>
	  			<%= form.check_box :Vegan, id: 12 %>
	  		</div>
	  	</div>
	  	<div class="col-md-4 checklist">
	  		<div class="row">
	  			<h4>Activites</h4>
	  		</div>
	  		<div class="row">
	  			<%= form.label :athletic %>
	  			<%= form.check_box :athletic, id: 13 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :Relaxed_Fun %>
	  			<%= form.check_box :Relaxed_Fun, id: 14 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :Day_Trip %>
	  			<%= form.check_box :Day_Trip, id: 15 %>
	  		</div>
	  		<div class="row">
	  			<%= form.label :Free %>
	  			<%= form.check_box :Free, id: 16 %>
	  		</div>
	  	</div>
	  </div>

	  <div class="actions row">
	    <%= form.submit %>
	  </div>
	<% end %>
</div>